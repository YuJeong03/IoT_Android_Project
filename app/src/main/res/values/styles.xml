<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#D9E5FF</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SplashTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>


    <style name="Theme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorAccent">#FDC2A9</item>
        <item name="colorPrimary">#FDC2A9</item>                    <!-- 하단 움직이는 바 색-->
        <item name="colorControlActivated">#7986CB</item>
    </style>

</resources>
